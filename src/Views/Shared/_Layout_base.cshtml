<!DOCTYPE html>
@RenderSection("topOfPage", required: false)
<!--[if lt IE 9]><html class="lte-ie8" lang="@(ViewData["htmlLang"] ?? "en")"><![endif]-->
<!--[if gt IE 8]><!--><html lang="@(ViewData["htmlLang"] ?? "en")"><!--<![endif]-->
  <head>
    <meta charset="utf-8" />
    <title>@(ViewBag.Title ?? "Welcome to GOV.UK")</title>

    <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="/css/govuk-template.css"/><!--<![endif]-->
    <!--[if IE 6]><link rel="stylesheet" media="screen" href="/css/govuk-template-ie6.css"/><![endif]-->
    <!--[if IE 7]><link rel="stylesheet" media="screen" href="/css/govuk-template-ie7.css"/><![endif]-->
    <!--[if IE 8]><link rel="stylesheet" media="screen" href="/css/govuk-template-ie8.css"/><![endif]-->
    <link rel="stylesheet" media="print" href="/css/govuk-template-print.css"/>

    <!--[if IE 8]><link rel="stylesheet" media="all" href="/css/fonts-ie8.css"/><![endif]-->
    <!--[if gte IE 9]><!--><link rel="stylesheet" media="all" href="/css/fonts.css"/><!--<![endif]-->
    <!--[if lt IE 9]><script src="/javascripts/ie.js"></script><![endif]-->

    <!--[if gt IE 8]><!--><link href="/css/govuk-elements-styles.css"  rel="stylesheet" type="text/css"><!--<![endif]-->
    <!--[if IE 6]><link href="/css/govuk-elements-styles-ie6.css" rel="stylesheet" type="text/css" /><![endif]-->
    <!--[if IE 7]><link href="/css/govuk-elements-styles-ie7.css" rel="stylesheet" type="text/css" /><![endif]-->
    <!--[if IE 8]><link href="/css/govuk-elements-styles-ie8.css" rel="stylesheet" type="text/css" /><![endif]-->

    <link rel="stylesheet" href="~/css/govuk-pagination.css"/>
    <link rel="stylesheet" href="~/css/govuk-application-site-search.css"/>
    <link rel="stylesheet" href="~/css/site.css"/>
    <link rel="shortcut icon" href="~/images/favicon.ico" type="image/x-icon" />

    <link rel="mask-icon" href="~/images/gov.uk_logotype_crown.svg" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="180x180" href="~/images/apple-touch-icon-180x180.png">
    <link rel="apple-touch-icon" sizes="167x167" href="~/images/apple-touch-icon-167x167.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/images/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" href="~/images/apple-touch-icon.png">


    <meta name="theme-color" content="#0b0c0c" />

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="~/images/opengraph-image.png">

    @RenderSection("head", required: false)
  </head>

  <body class="@ViewData["bodyClasses"]">
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

    @RenderSection("bodyStart", required: false)

    <div id="skiplink-container">
      <div>
        <a href="#content" class="skiplink">@(ViewData["skipLinkMessage"] ?? "Skip to main content")</a>
      </div>
    </div>

    <div id="global-cookie-message">
        @if (IsSectionDefined("cookieMessage")) {
            RenderSection("cookieMessage");
        }
        else {
            <p>GOV.UK uses cookies to make the site simpler. <a href="https://www.gov.uk/help/cookies">Find out more about cookies</a></p>
        }
    </div>


    <header role="banner" id="global-header" class="@ViewData["headerClass"]">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a href="@(ViewData["homepageUrl"] ?? "https://www.gov.uk")" title="@(ViewData["logoLinkTitle"] ?? "Go to the GOV.UK homepage")" id="logo" class="content">
              <img src="~/images/gov.uk_logotype_crown_invert_trans.png" width="36" height="32" alt=""> @(ViewData["globalHeaderText"] ?? "GOV.UK")
            </a>
          </div>
          @RenderSection("insideHeader", required: false)
        </div>
        @RenderSection("propositionHeader", required: false)
      </div>
    </header>


    @RenderSection("afterHeader", required: false)

    <div id="global-header-bar"></div>

    @RenderBody()

    <footer class="group js-footer" id="footer" role="contentinfo">

      <div class="footer-wrapper">
        @RenderSection("footerTop", required: false)

        <div class="footer-meta">
          <div class="footer-meta-inner">
            @RenderSection("footerSupportLinks", required: false)

            <div class="open-government-licence">
              <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>
              @if (IsSectionDefined("licenseMessage")) {
                @RenderSection("licenseMessage")
              } else {
                <p>All content is available under the <a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated</p>
              }
            </div>
          </div>

          <div class="copyright">
            <a href="https://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">@(ViewData["crownCopyrightMessage"] ?? "Â© Crown copyright")</a>
          </div>
        </div>
      </div>
    </footer>

    <div id="global-app-error" class="app-error hidden"></div>

    <script src="~/javascripts/govuk-template.js"></script>

    @RenderSection("bodyEnd", required: false)


    <script>if (typeof window.GOVUK === 'undefined') document.body.className = document.body.className.replace('js-enabled', '');</script>
  </body>
</html>
